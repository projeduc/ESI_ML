\mode<presentation>

%\definecolor{beamer@darkpurple0}{RGB}{100,0,150}
\definecolor{beamer@olivegreen}{HTML}{006600}
\definecolor{beamer@violet}{HTML}{6600CC}

\setbeamercolor{block title}{fg=beamer@violet,bg=white}
\setbeamercolor{block title alerted}{fg=red,bg=white}
\setbeamercolor{block title example}{fg=beamer@olivegreen,bg=white}

\setbeamercolor{block body}{parent=normal text, bg=white}
\setbeamercolor{block body alerted}{parent=normal text, bg=white}
\setbeamercolor{block body example}{parent=normal text, bg=white}


\setbeamercolor{title}{fg=beamer@violet,bg=white}
\setbeamercolor{subtitle}{parent=title}
\setbeamercolor{author}{parent=title}

\setbeamercolor*{palette primary}{fg=beamer@violet,bg=white}
\setbeamercolor*{palette secondary}{fg=white,bg=beamer@violet}
\setbeamercolor*{palette tertiary}{fg=white,bg=beamer@violet}
\setbeamercolor*{palette quaternary}{fg=white,bg=black}

\setbeamercolor*{sidebar}{use=structure,bg=structure.fg}

\setbeamercolor*{palette sidebar primary}{fg=beamer@violet}
\setbeamercolor*{palette sidebar secondary}{fg=beamer@violet}
\setbeamercolor*{palette sidebar tertiary}{fg=beamer@violet}
\setbeamercolor*{palette sidebar quaternary}{fg=beamer@violet}

\setbeamercolor*{titlelike}{parent=palette primary}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\setbeamercolor*{palette sidebar quaternary}{fg=beamer@violet}


\setbeamercolor{section in head/foot}{parent=palette primary}
\setbeamercolor{subsection in head/foot}{parent=palette primary}


\setbeamercolor{author in head/foot}{parent=palette primary}
\setbeamercolor{title in head/foot}{parent=palette primary}
\setbeamercolor{page number in head/foot}{parent=palette primary}

\setbeamercolor{frametitle}{parent=subsection in head/foot}
\setbeamercolor{frametitle right}{parent=section in head/foot}

\setbeamercolor{section title}{fg=beamer@violet}


\mode
<all>
